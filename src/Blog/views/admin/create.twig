{% extends '@admin/layout.twig' %}

{% block title "Editer l'article" %}

{% block body %}
    <h1>Ajouter un article {{ item.name }}</h1>
    <form action="{{ path('blog.admin.create', {id: item.id}) }}" method="post">
        {% include '@blog/admin/form.twig' %}
        <button class="btn btn-primary" type="submit">Ajouter l'article</button>

    </form>
{% endblock %}